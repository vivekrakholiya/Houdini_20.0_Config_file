  <tool name="tool_6" label="Rizom Bridge" icon="PLASMA_App">
    <script scriptType="python"><![CDATA[import tempfile, os, hou
from OD import rizomBridge
###########################################################################
#  Change the RizomUV path to your location                               #
###########################################################################
rizomPath = os.getenv('rizomuvpath')
################## DONT TOUCH ANYTHING BELOW THIS LINE ####################

exportFile = tempfile.gettempdir() + os.sep + "odrizombridge.obj"

res = hou.ui.displayMessage("OD Rizom Bridge\nby Oliver Hotz @ oliver@heimlich.net", buttons=('Send To Rizom','Send To Rizom (Clear UVs)','Get from Rizom', 'Automatic Roundtrip', 'Cancel'))
if res == 0:
    rizomBridge.sendToRizom(rizomPath = rizomPath, exportFile = exportFile)
if res == 1:
    rizomBridge.sendToRizomClearUvs(rizomPath = rizomPath, exportFile = exportFile)
if res == 2:
    rizomBridge.getFromRizom(None, rizomPath = rizomPath, exportFile = exportFile)
if res == 3:
    rizomBridge.automaticRoundtrip(rizomPath = rizomPath, exportFile = exportFile)
   ]]></script>
  </tool>

