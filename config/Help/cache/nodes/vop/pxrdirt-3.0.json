{"type": "root", "attrs": {"type": "node", "context": "vop", "tags": "rman,pattern", "icon": "NETWORKS/shop", "internal": "pxrdirt", "version": "3.0", "namespace": null}, "body": [{"level": 0, "type": "title", "indent": 0, "text": ["pxrdirt"], "extent": [68, 80]}, {"level": 1, "id": "parameters", "container": true, "type": "parameters_section", "indent": 0, "role": "section", "extent": [80, 93], "body": [{"type": "parameters_item_group", "body": [{"type": "parameters_item", "indent": 4, "text": ["Occluded"], "extent": [93, 108], "body": [{"type": "para", "indent": 7, "text": ["Occluded shading color. The default is white."], "extent": [135, 189]}], "container": true, "attrs": {"channel": "/occluded"}, "role": "item"}], "container": true, "role": "item_group"}], "text": "Parameters"}, {"type": "para", "indent": 0, "text": ["occluded (color)"], "extent": [189, 207], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Unoccluded"], "extent": [207, 223], "body": [{"type": "para", "indent": 7, "text": ["Unoccluded shading color. The default is black."], "extent": [252, 308]}], "container": true, "attrs": {"channel": "/unoccluded"}}], "container": true}], "container": true}, {"type": "para", "indent": 0, "text": ["unoccluded (color)"], "extent": [308, 328], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Num Samples"], "extent": [328, 345], "body": [{"type": "para", "indent": 7, "text": ["Controls the number of occlusion samples per camera hit point. The default is 4."], "extent": [374, 463]}], "container": true, "attrs": {"channel": "/numSamples"}}], "container": true}], "container": true}, {"type": "para", "indent": 0, "text": ["numSamples (int)"], "extent": [463, 481], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Sample Distribution"], "extent": [481, 506], "body": [{"type": "para", "indent": 7, "text": ["Sample distribution can be uniform or cosine. The defaul is cosine."], "extent":