{"type": "root", "attrs": {"type": "node", "context": "vop", "tags": "rman,pattern", "icon": "NETWORKS/shop", "internal": "pxrcolorcorrect", "version": "3.0", "namespace": null}, "body": [{"level": 0, "type": "title", "indent": 0, "text": ["pxrcolorcorrect"], "extent": [68, 88]}, {"level": 1, "id": "parameters", "container": true, "type": "parameters_section", "indent": 0, "role": "section", "extent": [88, 101], "body": [{"type": "parameters_item_group", "body": [{"type": "parameters_item", "indent": 4, "text": ["Input Color"], "extent": [101, 119], "body": [{"type": "para", "indent": 7, "text": ["Plug an input color pattern here."], "extent": [146, 188]}], "container": true, "attrs": {"channel": "/inputRGB"}, "role": "item"}], "container": true, "role": "item_group"}], "text": "Parameters"}, {"type": "para", "indent": 0, "text": ["inputRGB (color)"], "extent": [188, 206], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Input Mask"], "extent": [206, 222], "body": [{"type": "para", "indent": 7, "text": ["A mask defining the color-corrected areas."], "extent": [250, 301]}], "container": true, "attrs": {"channel": "/inputMask"}}], "container": true}], "container": true}, {"type": "para", "indent": 0, "text": ["inputMask (float)"], "extent": [301, 320], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Invert Mask"], "extent": [320, 337], "body": [{"type": "para", "indent": 7, "text": ["Inverts the mask\u2019s influence."], "extent": [366, 404]}], "container": true, "attrs": {"channel": "/invertMask"}}], "container": true}], "container": true}, {"type": "para", "indent": 0, "text": ["invertMask (int)"], "extent": [404, 422], "body": [{"type": "dt_group", "body": [{"type": "dt", "indent": 4, "text": ["Mix Mask"], "extent": [422, 436], "body": [{"type": "para", "indent": 7, "text": ["Blend in the mask. When set to 0.0, there is no color correction at all."], "extent": [462, 543]}], "container": true, "attrs": {"chann