numwheels.parm =
{
    LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)

    PARM_LABEL_SIMPLE("Number of Wheels", "numwheels") WIDTH(1.5);
    { WIDTH(0.1) }

    ROW
    {
	HSTRETCH(0.5) JUSTIFY(left, center) VALUE(numwheels.val)

        numwheels.field = PARM_FIELD (integer, "numwheels")
				VALUE(numwheels.val) HSTRETCH(0.5)
				 PREF_WIDTH(0.5);

        ROW
        {
	     LOOK(lightline) VMARGIN(1p)

   	     INCREMENT_ICONBUTTON VALUE(numwheels.more.val)	
	    	HELP("Add Another Instance") SIZE(0.2, 0.24);
	     DECREMENT_ICONBUTTON VALUE(numwheels.less.val)	
	        HELP("Remove Last Instance") SIZE(0.2, 0.24);

	}

	ACTION_BUTTON "Clear" VALUE(numwheels.clear.val);
    }

    { HSTRETCH }
}

numwheels.multilist = 
{
    LAYOUT(vertical) HSTRETCH MARGIN(0.05)
}

numwheels.gad =
{
    LAYOUT(vertical) HSTRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE
    SWITCHER tight
    {
	LAYOUT(overlap!)
        VALUE(numwheels.multilist_hide_buttons.val)
	{
	    LAYOUT(vertical) HSTRETCH
	    numwheels.parm;
	    {LOOK(none) HEIGHT(0.02)}
	}
	{
	}
    }
    numwheels.multilist;
}

