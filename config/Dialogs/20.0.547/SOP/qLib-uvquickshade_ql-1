            HMARGIN(0) VMARGIN(2p) SPACING(0, 2p)
	    bump.gad;		
	    bumpscale.gad;		

	    { STRETCH(1,1) }
	}

	TAB "..: Environment"
	{
	    LAYOUT(vertical) STRETCH

            HMARGIN(0) VMARGIN(2p) SPACING(0, 2p)
	    env.gad;		
	    envmap.gad;		
	    envscale.gad;		
	    envrough.gad;		
	    envrotate.gad;		
	    note_envmap_specular.gad;		

	    { STRETCH(1,1) }
	}
    } 
}

