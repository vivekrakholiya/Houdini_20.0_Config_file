    }

    {
	LAYOUT(vertical) MARGIN(2p) SPACING(0.02) HSTRETCH

	dissipationRamp.parm;

	SWITCHER tight
	{
	    LAYOUT(overlap) VALUE(dissipationRamp.hideparms.val) HSTRETCH
