indirectemissionfilter.row.join =		
{

  LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)
  HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

indirectemissionfilter.gad = ROW
{
    HSTRETCH JUSTIFY(left, center)

    {  }

    indirectemissionfilter.label = PARM_LABEL("Pixel Filter", "indirectemissionfilter") WIDTH(1.5);
    PARM_FIELD(string, "indirectemissionfilter") VALUE(indirectemissionfilter.val) 
	LABELREF(indirectemissionfilter.label) HSTRETCH;

    indirectemissionfilter.menu SIZE(0.26, 0.26) LOOK(menuiconbutton);

    {  }
}

