albedofilter.row.join =		
{

  LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)
  HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

albedofilter.gad = ROW
{
    HSTRETCH JUSTIFY(left, center)

    {  }

    albedofilter.label = PARM_LABEL("Pixel Filter", "albedofilter") WIDTH(1.5);
    PARM_FIELD(string, "albedofilter") VALUE(albedofilter.val) 
	LABELREF(albedofilter.label) HSTRETCH;

    albedofilter.menu SIZE(0.26, 0.26) LOOK(menuiconbutton);

    {  }
}

