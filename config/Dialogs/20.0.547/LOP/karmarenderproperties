hituvfilter.row.join =		
{

  LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)
  HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

hituvfilter.gad = ROW
{
    HSTRETCH JUSTIFY(left, center)

    {  }

    hituvfilter.label = PARM_LABEL("Pixel Filter", "hituvfilter") WIDTH(1.5);
    PARM_FIELD(string, "hituvfilter") VALUE(hituvfilter.val) 
	LABELREF(hituvfilter.label) HSTRETCH;

    hituvfilter.menu SIZE(0.26, 0.26) LOOK(menuiconbutton);

    {  }
}

