{
    LAYOUT(horizontal) JUSTIFY(left, center) 
    {  }

    hituvprecision.label = PARM_LABEL_SIMPLE("Precision", "hituvprecision") ;
    { WIDTH(0.09) }

    hituvprecision.menu;

    {  }

    {  }
}
