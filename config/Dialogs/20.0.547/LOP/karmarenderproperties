tonemapcurve.si_rampedit = FLOAT_RAMP VALUE(tonemapcurve.ramp_val)
			LAYOUT(horizontal)  HSTRETCH;

tonemapcurve.multiramp = 
{
    LAYOUT(vertical) STRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

    ROW
    {
	JUSTIFY(left,center) SPACING(0.1) VMARGIN(0.02) HSTRETCH

	COL
	{
	    JUSTIFY(right,center) WIDTH(1.5)
	    LABEL "Point No.";
	}

	tonemapcurve.pointno.gad
	    = INT_SLIDER_FIELD VALUE(tonemapcurve.pointno.val) HSTRETCH;
    }
}

tonemapcurve.buttons.gad = COL
{
    VSTRETCH

    ICON_BUTTON_STRIP
    {
	LAYOUT(horizontal) INITSIZE(20p, 20p) 

	ACTION_ICONBUTTON "BUTTONS_delete"
		HELP("Remove Point") VALUE(tonemapcurve.delpoint.val);
	ACTION_ICONBUTTON "BUTTONS_add"
		HELP("Add Point") VALUE(tonemapcurve.addpoint.val);
    }

    { VSTRETCH }

    ROW
    {
	    tonemapcurve.toggleparms.gad
	      = ACTION_ICONBUTTON "BUTTONS_show_controls"
		HELP("Hide Controls", "h.pane.parms.toggle_ramp_parms")
		VALUE(tonemapcurve.toggle.val)
		LOOKFLAT UI_FLAT_ICON_BTN_SIZE_TINY;

	    tonemapcurve.togglerampsize.gad
	      = ACTION_ICONBUTTON "BUTTONS_restore_mini"
		HELP("Maximize ramp", "h.pane.parms.toggle_ramp_size")
		VALUE(tonemapcurve.togglerampsize.val)
		LOOKFLAT UI_FLAT_ICON_BTN_SIZE_TINY;
    }
}

tonemapcurve.parm =
{
    LAYOUT(vertical) HSTRETCH JUSTIFY(left, center)

    tonemapcurve.ramp.gad = ROW
    {
	MARGIN(0.02) SPACING(0.02) JUSTIFY(left, top) HSTRETCH

	tonemapcurve.buttons.gad VSTRETCH;

	tonemapcurve.si_rampedit HSTRETCH;
    }
}

tonemapcurve.gad =
{
    LAYOUT(vertical) MARGIN(2p) HSTRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

    {
	LAYOUT(horizontal) HSTRETCH

	PARM_LABEL_SIMPLE("Tonemap Curve", "tonemapcurve");

	{ HSTRETCH }
	ACTION_ICONBUTTON "BUTTONS_ramp_flip_x" VALUE(tonemapcurve.reverse.val)
		LOOKFLAT HELP("Reverse Domain") UI_FLAT_ICON_BTN_SIZE_SMALL;
	ACTION_ICONBUTTON "BUTTONS_ramp_flip_y" VALUE(tonemapcurve.complement.val)
		LOOKFLAT HELP("Complement Ramp") UI_FLAT_ICON_BTN_SIZE_SMALL;
	tonemapcurve.interp
	    = ACTION_MENU_ICONBUTTON "BUTTONS_curve_segment_makecurved"
		LOOKFLAT HELP("Set Interpolation on All Points") UI_FLAT_ICON_BTN_SIZE_SMALL;
	tonemapcurve.presets
	    = ACTION_MENU_ICONBUTTON "BUTTONS_gear"
		LOOKFLAT HELP("Presets") UI_FLAT_ICON_BTN_SIZE_SMALL;

    }

    {
	LAYOUT(vertical) MARGIN(2p) SPACING(0.02) HSTRETCH

	tonemapcurve.parm;

	SWITCHER tight
	{
	    LAYOUT(overlap) VALUE(tonemapcurve.hideparms.val) HSTRETCH
	    tonemapcurve.multiramp;
	    {}
	}
    }
}

