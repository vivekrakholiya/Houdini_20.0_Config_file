extrarendervars.parm =
{
    LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)

    PARM_LABEL_SIMPLE("Render Vars", "extrarendervars") WIDTH(1.5);
    { WIDTH(0.1) }

    ROW
    {
	HSTRETCH(0.5) JUSTIFY(left, center) VALUE(extrarendervars.val)

        extrarendervars.field = PARM_FIELD (integer, "extrarendervars")
				VALUE(extrarendervars.val) HSTRETCH(0.5)
				 PREF_WIDTH(0.5);

        ROW
        {
	     LOOK(lightline) VMARGIN(1p)

   	     INCREMENT_ICONBUTTON VALUE(extrarendervars.more.val)	
	    	HELP("Add Another Instance") SIZE(0.2, 0.24);
	     DECREMENT_ICONBUTTON VALUE(extrarendervars.less.val)	
	        HELP("Remove Last Instance") SIZE(0.2, 0.24);

	}

	ACTION_BUTTON "Clear" VALUE(extrarendervars.clear.val);
    }

    { HSTRETCH }
}

extrarendervars.multilist = 
{
    LAYOUT(vertical) HSTRETCH MARGIN(0.05)
}

extrarendervars.gad =
{
    LAYOUT(vertical) HSTRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE
    SWITCHER tight
    {
	LAYOUT(overlap!)
        VALUE(extrarendervars.multilist_hide_buttons.val)
	{
	    LAYOUT(vertical) HSTRETCH
	    extrarendervars.parm;
	    {LOOK(none) HEIGHT(0.02)}
	}
	{
	}
    }
    extrarendervars.multilist;
}

