    HSTRETCH JUSTIFY(left, center)

    {  }

    motionvectorsfilter.label = PARM_LABEL("Motion Vectors", "motionvectorsfilter") WIDTH(1.32);
    PARM_FIELD(string, "motionvectorsfilter") VALUE(motionvectorsfilter.val) 
	LABELREF(motionvectorsfilter.label) HSTRETCH;

    motionvectorsfilter.menu SIZE(0.26, 0.26) LOOK(menuiconbutton);

    {  }
}
}	

