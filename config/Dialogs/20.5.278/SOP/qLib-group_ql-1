oplist.parm =
{
    LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)

    PARM_LABEL_SIMPLE("no.# Operations", "oplist") WIDTH(1.5);
    { WIDTH(0.1) }

    ROW
    {
	HSTRETCH(0.5) JUSTIFY(left, center) VALUE(oplist.val)

        oplist.field = PARM_FIELD (integer, "oplist")
				VALUE(oplist.val) HSTRETCH(0.5)
				 PREF_WIDTH(0.5);

        ROW
        {
	     LOOK(lightline) VMARGIN(1p)

   	     INCREMENT_ICONBUTTON VALUE(oplist.more.val)	
	    	HELP("Add Another Instance") SIZE(0.2, 0.24);
	     DECREMENT_ICONBUTTON VALUE(oplist.less.val)	
	        HELP("Remove Last Instance") SIZE(0.2, 0.24);

	}

	ACTION_BUTTON "Clear" VALUE(oplist.clear.val);
    }

    { HSTRETCH }
}

oplist.multilist = 
{
    LAYOUT(vertical) HSTRETCH MARGIN(0.05)
}

oplist.gad =
{
    LAYOUT(vertical) HSTRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE
    SWITCHER tight
    {
	LAYOUT(overlap!)
        VALUE(oplist.multilist_hide_buttons.val)
	{
	    LAYOUT(vertical) HSTRETCH
	    oplist.parm;
	    {LOOK(none) HEIGHT(0.02)}
	}
	{
	}
    }
    oplist.multilist;
}
